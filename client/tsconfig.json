{
  "compilerOptions": {
    "incremental": true,
    "target": "es2017",
    "jsx": "react",
    "strict": true,
    "outFile": "client.js",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}
